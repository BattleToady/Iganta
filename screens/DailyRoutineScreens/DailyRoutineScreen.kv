<DailyRoutineScreen>:
	BoxLayout:
        canvas.before:
            Color:
                rgba: 1, 1, 1, 1
            Rectangle:
                pos: self.pos
                size: self.size

        ScrollView:
            BoxLayout:
                size_hint: 1, 1.7
                orientation: 'vertical'

                Button:
                    size_hint: 1, None
                    height: 35
                    text: 'To-Do List'
                    on_press: app.get_running_app().root.current = 'ToDoListScreen'

                BoxLayout:

                